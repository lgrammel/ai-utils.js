{
  "name": "ai-utils.js",
  "description": "A TypeScript-first library for building AI apps, chatbots, and agents.",
  "version": "0.0.31",
  "author": "Lars Grammel",
  "license": "MIT",
  "keywords": [
    "llm",
    "embedding",
    "openai",
    "cohere",
    "huggingface",
    "stability-ai",
    "gpt-3",
    "gpt-4",
    "whisper",
    "chatbot",
    "ai"
  ],
  "homepage": "https://ai-utils.dev/",
  "repository": {
    "type": "git",
    "url": "https://github.com/lgrammel/ai-utils.js"
  },
  "bugs": {
    "url": "https://github.com/lgrammel/ai-utils.js/issues"
  },
  "type": "module",
  "sideEffects": false,
  "private": false,
  "engines": {
    "node": ">=18"
  },
  "files": [
    "./*"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs"
    }
  },
  "scripts": {
    "clean": "rimraf build dist",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --outDir dist/",
    "build:cjs": "tsc --outDir build/cjs/ -p tsconfig.cjs.json && node bin/prepare-cjs.js",
    "dist:copy-files": "copyfiles package.json README.md LICENSE dist",
    "dist": "npm run clean && npm run build && npm run dist:copy-files"
  },
  "dependencies": {
    "eventsource-parser": "1.0.0",
    "js-tiktoken": "1.0.7",
    "nanoid": "4.0.2",
    "secure-json-parse": "2.7.0",
    "zod": "3.21.4",
    "zod-to-json-schema": "3.21.4"
  },
  "devDependencies": {
    "@pinecone-database/pinecone": "^0.1.6",
    "@tsconfig/recommended": "1.0.2",
    "@types/node": "20.4.2",
    "copyfiles": "2.4.1",
    "rimraf": "5.0.1",
    "typescript": "5.1.6"
  },
  "peerDependencies": {
    "@pinecone-database/pinecone": "*"
  },
  "peerDependenciesMeta": {
    "@pinecone-database/pinecone": {
      "optional": true
    }
  }
}
